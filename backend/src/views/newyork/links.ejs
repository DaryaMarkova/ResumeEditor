<% if (links && links.length > 0) { %>
<h3 style="margin-top: 18px; margin-bottom: 6px; font-size: small">Links</h3>
<div class="template-linkslist">
  <% links.forEach(function(link) { %> <% if (link.label && link.href) { %>
  <a href="<%= link.href %>" class="template-linkslist__item">
    <%=link.label %>
  </a>
  <% } %> <% }); %>
</div>
<% } %>

<style>
  .template-linkslist a {
    display: block;
    color: rgb(33, 150, 243);
    text-decoration: none;
    font-size: x-small;
  }
</style>
